include: ../../usaco.def

page: circlecross_bronze, basic_frame
    module: cpp
    namespace: std

    &call open_In
        $my char L[100]
        fscanf(In, "%s", L)
        $dump L

    $my int A[26], int B[26]
    $for i=0:26
        A[i]=-1
    $for i=0:52
        c = L[i]-'A'
        $if A[c]<0
            A[c]=i
        $else
            B[c]=i

    $my int count=0
    $for i=0:26
        $for j=i+1:26
            # : Ai--Bi Aj--Bj Ai<Bi Aj<Bj
            # Ai Aj Bi Bj
            # Aj Ai Bj Bi
            $if (A[i]<A[j] && A[j]<B[i] && B[i]<B[j]) || (A[j]<A[i] && A[i]<B[j] && B[j]<B[i])
                count++

    $dump count
    &call open_Out
        $print $count


