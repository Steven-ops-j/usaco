import java.util.*;
import java.io.*;

public class factory{
        public static void main(String[] args)throws IOException{
                Scanner scan = new Scanner(new File("factory.in"));
                int N = scan.nextInt();
                int[] a = new int[N];
                int[] b = new int[N];
                int temp=0;
                for(int i=0; i<N-1; i++){
                        a[i] = scan.nextInt();
                        b[i] = scan.nextInt();
                        if(a[i]>b[i]){
                                temp=b[i];
                                b[i]=a[i];
                                a[i]=temp;
                        }
                }
                scan.close();

                int count=0, ans=-1;
                for(int i=0; i<N-1; i++){
                        for(int j=0; j<2; j++){
                                if(a[i]==b[j])
                                        count++;
                        }
                        if(count==N-2)
                                ans=a[i];
			
                }

                PrintWriter out = new PrintWriter(new BufferedWriter(new FileWriter("factory.out")));
                out.println(ans);
                out.close();

                //System.out.println(ans);
        }
}